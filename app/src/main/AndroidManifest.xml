        </activity>
        <activity
            android:name=".Control.Main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.AdvancedTuning.AdvancedTuningActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.InputLevel.InputLevelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.OutputSetting.OutputSettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.Preferences.PreferencesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.SetupWizard.SetupWizardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Control.ManagerPreset.ManagerPresetActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>