<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.haoke.bhzs.Control.AdvancedTuning.CH_Level.CHLevelFragment2">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <com.haoke.bhzs.Utils.View.MyImageView
            android:layout_marginTop="@dimen/layout_y_8"
            android:layout_marginRight="@dimen/layout_x_30"
            android:layout_width="@dimen/layout_x_36"
            android:layout_height="@dimen/layout_x_40"
            android:layout_gravity="right"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@mipmap/save_normat"
            app:src_n="@mipmap/save_normat"
            app:src_s="@mipmap/save_selected" />

        <com.haoke.bhzs.Utils.View.MyView
            android:id="@+id/chlevel1_myView1"
            android:layout_width="@dimen/layout_x_180"
            android:layout_height="@dimen/layout_x_180"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/layout_y_15"
            android:background="@color/background"
            app:Max="100"
            app:nei_color="@color/colorLv"
            app:nei_normal_color="@color/colorBai"
            app:nei_roundWidth="4"
            app:wai_color="@color/colorLv"
            app:wai_normal_color="@color/colorHei"
            app:wai_roundWidth="8" />

        <LinearLayout
            android:layout_marginTop="@dimen/layout_y_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.haoke.bhzs.Utils.View.MyImageView
                android:id="@+id/chlevel1_myview1_jian"
                android:layout_marginLeft="@dimen/layout_x_100"
                android:layout_width="@dimen/layout_x_30"
                android:layout_height="@dimen/layout_x_30"
                android:layout_gravity="left"
                android:clickable="true"
                android:scaleType="fitXY"
                android:src="@mipmap/decrease_normat"
                app:src_n="@mipmap/decrease_normat"
                app:src_s="@mipmap/decrease_pressed" />


            <TextView
                android:layout_marginLeft="@dimen/layout_x_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tweeter FL"
                android:textColor="@color/bai"
                android:textSize="@dimen/layout_x_20" />

            <LinearLayout
                android:layout_marginLeft="@dimen/layout_x_15"
                android:layout_width="@dimen/layout_x_66"
                android:layout_height="@dimen/layout_y_28"
                android:paddingBottom="@dimen/layout_x_2"
                android:paddingRight="@dimen/layout_x_3"
                android:paddingLeft="@dimen/layout_x_3"
                android:background="@color/color_111"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/chlevel1_myview1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-30"
                    android:textColor="@color/colorLv"
                    android:textSize="@dimen/layout_x_25" />

                <TextView
                    android:layout_marginLeft="@dimen/layout_x_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dB"
                    android:textColor="@color/color_aaa"
                    android:textSize="@dimen/layout_x_16" />
            </LinearLayout>

            <com.haoke.bhzs.Utils.View.MyImageView
                android:id="@+id/chlevel1_myview1_jia"
                android:layout_marginLeft="@dimen/layout_x_30"
                android:layout_width="@dimen/layout_x_30"
                android:layout_height="@dimen/layout_x_30"
                android:layout_gravity="center_vertical"
                android:clickable="true"
                android:scaleType="fitXY"
                android:src="@mipmap/increase_normat"
                app:src_n="@mipmap/increase_normat"
                app:src_s="@mipmap/increase_pressed" />
        </LinearLayout>

        <com.haoke.bhzs.Utils.View.MyImageView
            android:layout_marginTop="@dimen/layout_y_35"
            android:layout_width="@dimen/layout_x_36"
            android:layout_height="@dimen/layout_x_40"
            android:layout_gravity="center"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@mipmap/save_normat"
            app:src_n="@mipmap/save_normat"
            app:src_s="@mipmap/save_selected" />



        <com.haoke.bhzs.Utils.View.MyView
            android:id="@+id/chlevel1_myView2"
            android:layout_width="@dimen/layout_x_180"
            android:layout_height="@dimen/layout_x_180"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/layout_y_35"
            android:background="@color/background"
            app:Max="100"
            app:nei_color="@color/colorLv"
            app:nei_normal_color="@color/colorBai"
            app:nei_roundWidth="4"
            app:wai_color="@color/colorLv"
            app:wai_normal_color="@color/colorHei"
            app:wai_roundWidth="8" />

        <LinearLayout
            android:layout_marginTop="@dimen/layout_y_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.haoke.bhzs.Utils.View.MyImageView
                android:id="@+id/chlevel1_myview2_jian"
                android:layout_marginLeft="@dimen/layout_x_100"
                android:layout_width="@dimen/layout_x_30"
                android:layout_height="@dimen/layout_x_30"
                android:layout_gravity="left"
                android:clickable="true"
                android:scaleType="fitXY"
                android:src="@mipmap/decrease_normat"
                app:src_n="@mipmap/decrease_normat"
                app:src_s="@mipmap/decrease_pressed" />


            <TextView
                android:layout_marginLeft="@dimen/layout_x_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tweeter FR"
                android:textColor="@color/bai"
                android:textSize="@dimen/layout_x_20" />

            <LinearLayout
                android:layout_marginLeft="@dimen/layout_x_15"
                android:layout_width="@dimen/layout_x_66"
                android:layout_height="@dimen/layout_y_28"
                android:paddingBottom="@dimen/layout_x_2"
                android:paddingRight="@dimen/layout_x_3"
                android:paddingLeft="@dimen/layout_x_3"
                android:background="@color/color_111"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/chlevel1_myview2_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-30"
                    android:textColor="@color/colorLv"
                    android:textSize="@dimen/layout_x_25" />

                <TextView
                    android:layout_marginLeft="@dimen/layout_x_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="dB"
                    android:textColor="@color/color_aaa"
                    android:textSize="@dimen/layout_x_16" />
            </LinearLayout>

            <com.haoke.bhzs.Utils.View.MyImageView
                android:id="@+id/chlevel1_myview2_jia"
                android:layout_marginLeft="@dimen/layout_x_30"
                android:layout_width="@dimen/layout_x_30"
                android:layout_height="@dimen/layout_x_30"
                android:layout_gravity="center_vertical"
                android:clickable="true"
                android:scaleType="fitXY"
                android:src="@mipmap/increase_normat"
                app:src_n="@mipmap/increase_normat"
                app:src_s="@mipmap/increase_pressed" />
        </LinearLayout>

    </LinearLayout>



</RelativeLayout>
